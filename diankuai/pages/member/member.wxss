/*@import "../../utils/wxParse/wxParse.wxss";*/
page{
  background-color: #ddd;
  font-size: 32rpx;
}
.search{
  position: relative;
  padding:20rpx 10rpx;
  background-color: #fff;
}
.search-input{
  width: 85%;
  border: 1rpx solid #ddd;
  border-radius: 10rpx;
  padding: 0 16rpx;
  height: 60rpx;
}
.search-icon{
  position: absolute;
  right: 14%;
  top: 34rpx;
  z-index: 5;
}
.gouwuche{
  position: absolute;
  right: 20rpx;
  top: 30rpx;
  width: 50rpx;
  height: 50rpx;
}
.classify{
  display: flex;
  justify-content: space-between;
  padding: 0 0 0 40rpx;
  line-height: 1.6;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  border-top: 1rpx solid #ddd;
}
.classify-li{
  text-align: center;
  width: 100%;
}
.classify-li-hover{
  color: #f84900;
  border-bottom: 6rpx solid #f84900;
}
.icon-liebiao{
  position: relative;
  top: 4rpx;
  font-size: 40rpx;
}
.icon-fenlei{
  font-size: 40rpx;
}

.ul1{
  overflow: hidden;
}
.ul1 .li{
  margin-top: 2vw;
  width: 49%;

  float: left;
  background-color: #fff;
}
.ul1 .li:nth-child(2n){
  margin-left: 2%;
}
.ul1 .li-img{
  width: 100%;
  height: 40vw;
  margin: 50rpx 0 50rpx;
}
.ul1 .li-title{
  font-size: 38rpx;
  font-weight: bold;
  line-height: 1.4;
  height: 100rpx;
  padding: 0 10rpx;
}
.ul1 .money{
  padding: 20rpx 10rpx;
  line-height: 40rpx;
}
.ul1 .money1{
  font-size: 36rpx;
  color: #f84900;
}
.ul1 .money2{
  font-size: 28rpx;
  color: #ddd;
  float: right;
}

.ul2{
  margin-top: 2vw;
}
.ul2 .li{
  border-bottom: 1rpx solid #ddd;
  padding: 20rpx;
  border-bottom: 1px solid #ddd;
  overflow: hidden;
  background-color: #fff;
}
.ul2 .li-img{
  width:36vw;
  height: 26vw;
  float: left;
  padding-right: 20rpx;
}

.ul2 .li-title{
  font-size: 38rpx;
  font-weight: bold;
  height: 100rpx;
  padding: 0 10rpx;
}
.ul2 .money{
  padding: 20rpx 10rpx;
  line-height: 40rpx;
}
.ul2 .money1{
  font-size: 36rpx;
  color: #f84900;
}
.ul2 .money2{
  display: block;
  font-size: 28rpx;
  color: #ddd;
  margin-top: 10rpx;
}







