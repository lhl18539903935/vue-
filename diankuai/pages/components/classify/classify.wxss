/* pages/components/classify/classify.wxss */
.classify_ul{
  width: 100%;
  overflow: hidden;
}
.hidden{
  height: 76rpx;
}
  .classify_li{
    width: calc(25% - 10px);
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    float: left;
    box-sizing: border-box;
    margin: 10rpx;
  }
  .yuan{
    border-radius: 32rpx;
  }
  .btn{
    width: calc(25% - 20rpx);
    height: 56rpx;
    line-height: 56rpx;
    border-radius: 32rpx;
    text-align: center;
    float: right;
    box-sizing: border-box;
    margin: 10rpx;
  }